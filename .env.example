# Application
NODE_ENV=development
SITE_URL=https://localhost:5173

# Builder.io
BUILDER_PUBLIC_KEY=your_builder_public_key_here
BUILDER_PRIVATE_KEY=your_builder_private_key_here
BUILDER_WEBHOOK_SECRET=your_webhook_secret_here

# Database
DATABASE_URL=postgresql://devuser:devpass@localhost:5432/app_dev
DATABASE_URL_TEST=postgresql://devuser:devpass@localhost:5432/app_test

# Redis
REDIS_URL=redis://:devpass@localhost:6379
REDIS_TLS_URL=rediss://:devpass@localhost:6380

# Authentication
AUTH_SECRET=your_super_secret_key_here_minimum_32_chars
OAUTH_GOOGLE_ID=your_google_oauth_id
OAUTH_GOOGLE_SECRET=your_google_oauth_secret
OAUTH_GITHUB_ID=your_github_oauth_id
OAUTH_GITHUB_SECRET=your_github_oauth_secret

# Storage (S3/MinIO)
S3_ENDPOINT=http://localhost:9000
S3_ACCESS_KEY=minioadmin
S3_SECRET_KEY=minioadmin
S3_BUCKET=uploads
S3_REGION=us-east-1

# Email (Mailhog for dev)
SMTP_HOST=localhost
SMTP_PORT=1025
SMTP_USER=
SMTP_PASS=
SMTP_FROM=noreply@example.com

# Monitoring
SENTRY_DSN=https://your_sentry_dsn@sentry.io/123456
SENTRY_ENVIRONMENT=development
DATADOG_CLIENT_TOKEN=your_datadog_client_token
DATADOG_APPLICATION_ID=your_datadog_app_id

# Analytics
GA_TRACKING_ID=G-XXXXXXXXXX
MIXPANEL_TOKEN=your_mixpanel_token

# Feature Flags
FEATURE_NEW_DASHBOARD=true
FEATURE_BETA_EDITOR=false

# API Keys
OPENAI_API_KEY=sk-...
STRIPE_SECRET_KEY=sk_test_...
STRIPE_WEBHOOK_SECRET=whsec_...

# Deployment
DEPLOY_TARGET=cloudflare
CLOUDFLARE_ACCOUNT_ID=your_account_id
CLOUDFLARE_API_TOKEN=your_api_token