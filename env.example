# Supabase Configuration
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key
SUPABASE_SERVICE_KEY=your-service-key
SUPABASE_PROJECT_ID=your-project-id

# Builder.io Configuration
BUILDER_PUBLIC_KEY=your-builder-public-key

# Cloudflare Configuration
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id

# KV Namespace IDs (get these from Cloudflare dashboard)
KV_NAMESPACE_ID=your-kv-namespace-id
KV_PREVIEW_ID=your-kv-preview-namespace-id

# R2 Bucket
R2_BUCKET_NAME=your-app-storage

# Deployment Target (cloudflare, vercel, static, node)
DEPLOY_TARGET=cloudflare

# Optional: Qwik Insights
QWIK_INSIGHTS_KEY=your-qwik-insights-key

# Optional: Monitoring
SENTRY_DSN=your-sentry-dsn
DATADOG_CLIENT_TOKEN=your-datadog-client-token

# Development
ENVIRONMENT=development
